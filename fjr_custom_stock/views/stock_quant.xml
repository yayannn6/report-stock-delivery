<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_quant_view_list_inherit" model="ir.ui.view">
        <field name="name">stock.quant.view.list.inherit</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_inventory_editable"/>
        <field name="arch" type="xml">
            <field name="quantity" position="after">
                <field name="container_quantity" optional="hide"/>
            </field>

        </field>
    </record>

    <record id="stock_quant_view_list_editable_inherit" model="ir.ui.view">
        <field name="name">stock.quant.view.list.inherit</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"/>
        <field name="arch" type="xml">
            <field name="reserved_quantity" position="after">
                <field name="container_quantity" optional="hide"/>
            </field>
        </field>
    </record>



</odoo>
